{
  "Version": "2012-10-17",
  "Id": "westmarches-infra-policy",
  "Statement": [
    {
      "Sid": "Access Bucket",
      "Effect": "Allow",
      "Principal": {
        "SCW": [
          "project_id:395a81f2-0abe-417b-9e08-95079a607e2f"
        ]
      },
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": [
        "westmarches-infra"
      ]
    },
    {
      "Sid": "Manage objects",
      "Effect": "Allow",
      "Principal": {
        "SCW": "project_id:395a81f2-0abe-417b-9e08-95079a607e2f"
      },
      "Action": [
        "s3:GetObject",
        "s3:DeleteObject",
        "s3:PutObject",
        "s3:GetObjectVersion",
        "s3:GetObjectTagging"
      ],
      "Resource": [
        "westmarches-infra/*"
      ]
    }
  ]
}